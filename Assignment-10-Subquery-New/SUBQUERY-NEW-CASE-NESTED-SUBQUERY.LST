SELECT ENAME,HIREDATE,JOB,SAL,SAL*12,TO_CHAR(HIREDATE,'FMDAY')
FROM EMP
WHERE DEPTNO IN (SELECT DEPTNO
		 FROM EMP
		 WHERE ENAME='MARTIN')
AND TO_CHAR(HIREDATE,'YY') IN (81,82,85,87)
AND MGR IN (SELECT MGR
	    FROM EMP
	    WHERE ENAME='TURNER')
AND SAL > (SELECT MIN(SAL)
	   FROM EMP
	   WHERE SAL > 
(SELECT MIN(SAL)
	   FROM EMP
	   WHERE SAL > 
                       (SELECT MIN(SAL)
	   FROM EMP
	   )))

AND SAL <= (SELECT MAX(SAL)
	   FROM EMP
	   WHERE SAL < 
(SELECT MAX(SAL)
	   FROM EMP
	   WHERE SAL < 
                       (SELECT MAX(SAL)
	   FROM EMP
WHERE SAL < (SELECT MAX(SAL)
	     FROM EMP
	   ))))
AND INSTR(ENAME,1,2) > 0
AND TO_HAR(HIREDATE,'FMDAY') NOT IN ('SUNDAY','WEDNESDAY')

AND COMM < (SELECT MAX(COMM)
	    FROM EMP
	    WHERE COMM < (SELECT MAX(COMM)
			  FROM EMP
			  WHERE COMM < (SELECT MAX(COMM)
					FROM EMP
 					WHERE COMM < (SELECT MAX(COMM)
							FROM EMP))))
AND JOB NOT IN (SELECT JOB
		FROM EMP
		WHERE ENAME IN ('KING','SMITH','BLAKE')); 

/
